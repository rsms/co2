; should all parse identically
(Fun A (params nil) <nil> (body (Block
  (Var a (IntLit 1))
  (If (cond (BinOp < (Id a (Var a)) (IntLit 3)))
      (then (Assign (left (Var a)) (right (IntLit 5))) ))
  (Id a (Var a)) )))

